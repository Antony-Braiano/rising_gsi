From 001ab9fa881321c994dcbd54dbdf8fc503ef30bb Mon Sep 17 00:00:00 2001
From: naz664 <nazimnavas145@gmail.com>
Date: Sat, 3 Sep 2022 14:06:49 +0000
Subject: [PATCH 1/3] custom qs tiles combo

---
 packages/SystemUI/res/values/config.xml         | 2 +-
 packages/SystemUI/res/values/lineage_config.xml | 4 ++--
 2 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/packages/SystemUI/res/values/config.xml b/packages/SystemUI/res/values/config.xml
index 81cc56b7c0eb..d394ad159ad3 100644
--- a/packages/SystemUI/res/values/config.xml
+++ b/packages/SystemUI/res/values/config.xml
@@ -74,7 +74,7 @@
 
     <!-- The default tiles to display in QuickSettings -->
     <string name="quick_settings_tiles_default" translatable="false">
-        internet,bt,flashlight,dnd,alarm,airplane,nfc,rotation,battery,controls,wallet,cast,screenrecord,custom(com.google.android.gms/.nearby.sharing.SharingTileService)
+        wifi,bt,dnd,flashlight,sync,battery,cell,airplane,dark,nfc,rotation,screenrecord,hotspot,location,custom(com.google.android.gms/.nearby.sharing.SharingTileService)
     </string>
 
     <!-- The minimum number of tiles to display in QuickSettings -->
diff --git a/packages/SystemUI/res/values/lineage_config.xml b/packages/SystemUI/res/values/lineage_config.xml
index 6c92a4fb2a3c..69d006a887d3 100644
--- a/packages/SystemUI/res/values/lineage_config.xml
+++ b/packages/SystemUI/res/values/lineage_config.xml
@@ -26,13 +26,13 @@
     <bool name="config_fingerprintWakeAndUnlock">true</bool>
 
     <!-- Color of the UDFPS pressed view -->
-    <color name="config_udfpsColor">#ffffffff</color>
+    <color name="config_udfpsColor">#00ff00</color>
 
     <!-- HBM type of UDFPS overlay.
             0 - GLOBAL HBM
             1 - LOCAL HBM
     -->
-    <integer name="config_udfpsHbmType">1</integer>
+    <integer name="config_udfpsHbmType">0</integer>
 
     <!-- Udfps HBM provider class name -->
     <string name="config_udfpsHbmProviderComponent">com.android.systemui.biometrics.DummyUdfpsHbmProvider</string>
-- 
